INPUT

T_MARPA_G_C
	if (sv_isa($arg, \"Marpa::R2::Internal::G_C\")) {
	    IV tmp = SvIV((SV*)SvRV($arg));
	    G_Wrapper * g_wrapper = INT2PTR(G_Wrapper *, tmp);
	    $var = g_wrapper->g;
	} else
	    Perl_croak(aTHX_ \"%s: %s is not of type Marpa::R2::Internal::G_C\",
			${$ALIAS?\q[GvNAME(CvGV(cv))]:\qq[\"$pname\"]},
			\"$var\")

